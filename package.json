{
  "name": "starlight",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "@prisma/client": "^6.17.1",
      "prisma-json-types-generator": "^3.6.2",
      "@prisma/adapter-pg": "^6.17.1",
      "tsdown": "^0.15.5",
      "@orpc/server": "^1.9.0",
      "@orpc/openapi": "^1.9.0",
      "@orpc/client": "^1.9.0",
      "@orpc/zod": "^1.9.0",
      "zod": "^4.1.12",
      "prisma": "^6.17.1",
      "grammy": "^1.38.3",
      "hono": "^4.9.12"
    }
  },
  "scripts": {
    "lint": "biome check --write .",
    "check-types": "turbo check-types",
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F server dev",
    "start:server": "turbo -F server start",
    "start:web": "turbo -F web start",
    "db:push": "turbo -F @starlight/utils db:push",
    "db:generate": "turbo -F @starlight/utils db:generate",
    "db:migrate": "turbo -F @starlight/utils db:migrate",
    "queue:classify": "turbo -F server queue:classify",
    "queue:embeddings": "turbo -F server queue:embeddings"
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@types/bun": "latest",
    "oxlint-tsgolint": "^0.2.0",
    "tsdown": "^0.15.5",
    "turbo": "^2.5.9-canary.8",
    "ultracite": "5.6.4"
  },
  "packageManager": "bun@1.3.0",
  "engines": {
    "bun": ">=1.3.0"
  }
}
