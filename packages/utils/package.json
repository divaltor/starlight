{
  "name": "@starlight/utils",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "build": "bun run db:generate && bun run -b tsdown",
    "db:push": "prisma --config prisma.config.ts migrate deploy",
    "db:generate": "prisma --config prisma.config.ts generate --sql",
    "db:migrate": "prisma --config prisma.config.ts migrate dev"
  },
  "dependencies": {
    "@prisma/client": "catalog:",
    "@prisma/adapter-pg": "catalog:",
    "@t3-oss/env-core": "^0.13.8",
    "sqids": "^0.3.0",
    "uuid": "^13.0.0",
    "zod": "catalog:",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.2"
  },
  "devDependencies": {
    "prisma": "catalog:",
    "prisma-json-types-generator": "catalog:",
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
