{
  "name": "@starlight/api",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "bun run -b tsdown"
  },
  "devDependencies": {
    "tsdown": "catalog:",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@orpc/client": "catalog:",
    "@orpc/openapi": "catalog:",
    "@orpc/server": "catalog:",
    "@orpc/zod": "catalog:",
    "@starlight/crypto": "workspace:*",
    "@starlight/utils": "workspace:*",
    "@telegram-apps/init-data-node": "^2.0.10",
    "grammy": "catalog:",
    "hono": "catalog:",
    "ioredis": "catalog:",
    "zod": "catalog:",
    "date-fns": "^4.1.0"
  }
}
